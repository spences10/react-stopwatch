{
  "name": "react-stopwatch",
  "version": "0.1.80",
  "private": true,
  "scripts": {
    "dev": "parcel src/index.html -p 2020 --open",
    "build": "parcel build src/index.html",
    "clean": "rm -rf .cache && rm -rf dist",
    "format": "pretty-quick"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "babel-plugin-styled-components": "1.10.7",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-helmet": "6.1.0",
    "styled-components": "4.4.1"
  },
  "devDependencies": {
    "@babel/core": "7.15.0",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "husky": "4.3.8",
    "parcel-bundler": "1.12.5",
    "prettier": "2.3.2",
    "pretty-quick": "2.0.2"
  },
  "prettier": {
    "printWidth": 70,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "proseWrap": "always"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
