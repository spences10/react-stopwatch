{
  "name": "react-stopwatch",
  "version": "0.1.5",
  "private": true,
  "scripts": {
    "dev": "parcel src/index.html -p 2020 --open",
    "build": "parcel build src/index.html",
    "clean": "rm -rf .cache && rm -rf dist",
    "format": "pretty-quick"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "babel-plugin-styled-components": "1.10.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.0",
    "styled-components": "4.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "husky": "2.1.0",
    "parcel-bundler": "1.12.3",
    "prettier": "1.17.0",
    "pretty-quick": "1.10.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
